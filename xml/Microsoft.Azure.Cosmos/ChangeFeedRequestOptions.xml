    <Member MemberName="EmitOldContinuationToken">
      <MemberSignature Language="C#" Value="public bool EmitOldContinuationToken { get; set; }" />
      <MemberSignature Language="ILAsm" Value=".property instance bool EmitOldContinuationToken" />
      <MemberSignature Language="DocId" Value="P:Microsoft.Azure.Cosmos.ChangeFeedRequestOptions.EmitOldContinuationToken" />
